public class Peminjaman {
    int id, lamaPinjam, hargaHari;
    String nama, namaGame;
    
    Peminjaman(){
    }
        Peminjaman(int kd, String nm, String gm, int lm){
        id = kd;
        nama = nm;
        namaGame = gm;
        lamaPinjam = lm;
        
    }
        
     int hitungHarga(int hari){
         int hargaPerHari = 25000;
         return hari * hargaPerHari;
     }   
     void tampilPeminjaman(){
         int hargaPerHari = 25000;
         System.out.println("Id = " + id);
         System.out.println("Nama member = " + nama);
         System.out.println("Nama Game = " + namaGame);
         System.out.println("Lama meminjam = " + lamaPinjam + " hari");
     }
}
